## fastqc modulefile
##
## Initializes fastqc.
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use rar and unrar"
	puts stderr "\n\tVersion $version\n"
}

module-whatis "A compression and decompression utility."

# for Tcl script use only
set version 0.5.5.4

module load python/2.7.6
module load bedtools/2.25.0 
module load samtools/1.3
module load tabix/0.2.6  


prepend-path PATH $::env(MODULESAPPSDIR)/uscs_tools
