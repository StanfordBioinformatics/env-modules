#%Module1.0
#
## sqlite modulefile
##
## Initializes sqlite
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the sqlite package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes sqlite package"

# for Tcl script use only
set version 3.14.02

prepend-path PATH $::env(MODULESAPPSDIR)/sqlite/$version/bin
prepend-path LD_LIBRARY_PATH $::env(MODULESAPPSDIR)/sqlite/$version/lib
