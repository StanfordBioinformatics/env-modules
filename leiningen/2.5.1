#%Module1.0
#
## leiningen modulefile
##
## Initializes leiningen
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the leiningen package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes leiningen package"

# for Tcl script use only
set version 2.5.1

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/leiningen/$version
