#%Module1.0
#
## Subread modulefile
##
## Initializes Subread
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the Subread package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes RSeQC package"

# for Tcl script use only
set version 1.4.6

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/subread/$version/bin
