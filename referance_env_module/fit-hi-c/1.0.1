#%Module1.0
#
## fit-hi-c modulefile
##
## Initializes the fit-hi-c package for intra-chromosomal contact map confidence estimates
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the fit-hi-c package for intra-chromosomal contact map confidence estimates\n"
	puts stderr "\n\tVersion $version\n"
}

module-whatis "initializes the fit-hi-c package for intra-chromosomal contact map confidence estimates"

# for Tcl script use only
set version 1.0.1

# sets the MODULESAPPSDIR env var
module add modsappsdir
module add r/3.1.0
module add python/2.7

prepend-path PATH $::env(MODULESAPPSDIR)/fit-hi-c/$version/bin
