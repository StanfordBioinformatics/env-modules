#%Module1.0
#
## prodigal modulefile
##
## Initializes prodigal
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the prodigal package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes prodigal package"

# for Tcl script use only
set version 2.6.2

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/prodigal/$version
