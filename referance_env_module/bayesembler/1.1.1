#%Module1.0
#
## bayesembler modulefile
##
## Initializes bayesembler
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the bayesembler package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes bayesembler package"

# for Tcl script use only
set version 1.1.1

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/bayesembler/$version
