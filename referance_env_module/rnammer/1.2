#%Module1.0
#
## rnammer modulefile
##
## Initializes rnammer
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the rnammer package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes rnammer package"

# for Tcl script use only
set version 1.2

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/rnammer/$version
