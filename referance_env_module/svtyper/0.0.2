#%Module1.0
#
## svtyper modulefile
##
## Initializes svtyper
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the svtyper package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes svtyper package"

# for Tcl script use only
set version 0.0.2

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/svtyper/$version/svtyper
