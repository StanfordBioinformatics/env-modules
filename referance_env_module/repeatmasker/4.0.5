#%Module1.0
#
## repeatmasker modulefile
##
## Initializes repeatmasker
##
proc ModulesHelp { } {
global version

puts stderr "\tInitializes your environment to use the repeatmasker package"
puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes repeatmasker package"

# Add required modules

# for Tcl script use only.
set version 4.0.5

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/repeatmasker/4.0.5
