#%Module1.0
#
## eXpress modulefile
##
## Initializes eXpress
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the eXpress package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes eXpress package"

# for Tcl script use only
set version 1.5.1

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/express/$version
