#%Module1.0
#
## ete2 modulefile
##
## Initializes ete2
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the ete2 package"
	puts stderr "\n\tVersion $version\n"
}

module-whatis "initializes ete2 package"

# for Tcl script use only
set version 2.3.10

module add python/2.7
module add PyQt4/4.11.4

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/ete2/$version/bin
prepend-path PYTHONPATH $::env(MODULESAPPSDIR)/ete2/$version/lib/python2.7/site-packages
