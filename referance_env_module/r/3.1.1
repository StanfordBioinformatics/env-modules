#%Module1.0

proc ModulesHelp { } {
        puts stderr "\t use R for CLI"
        puts stderr "\t use library(); to see installed R packages"
        }

module-whatis   "Latest version of R"

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path	PATH			$::env(MODULESAPPSDIR)/R/R-3.1.1/bin
prepend-path	LD_LIBRARY_PATH		$::env(MODULESAPPSDIR)/R/R-3.1.1/lib64/R/lib
#prepend-path	R_LIBS			$::env(MODULESAPPSDIR)/R/R-3.1.1/lib64/R/library
