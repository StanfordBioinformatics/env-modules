#%Module1.0
#
## HiCorrector modulefile
##
## Initializes HiCorrector
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the HiCorrector package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes HiCorrector package"

# for Tcl script use only
set version 1.1

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/HiCorrector/$version/bin
