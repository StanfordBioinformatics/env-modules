#%Module1.0
#
proc ModulesHelp { } {
        puts stderr "\tInitializes your environment to use v8 java"
}

module-whatis "initializes java"

setenv JAVA_HOME /srv/gsfs0/software/java/jre1.8.0_66
prepend-path PATH /srv/gsfs0/software/java/jre1.8.0_66/bin
prepend-path JRE_HOME /srv/gsfs0/software/java/jre1.8.0_66/jre
prepend-path MANPATH /srv/gsfs0/software/java/jre1.8.0_66/man
