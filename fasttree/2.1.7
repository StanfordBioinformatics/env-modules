#%Module1.0
#
## fasttree modulefile
##
## Initializes fasttree
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the fasttree package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes fasttree package"

# for Tcl script use only
set version 2.1.7

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/fasttree/FastTree$version
