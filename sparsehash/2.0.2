#%Module1.0
#
## sparsehash modulefile
##
## Initializes sparsehash
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the sparsehash package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes sparsehash package"

# for Tcl script use only
set version 2.0.2

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/sparsehash/sparsehash-$version/include
