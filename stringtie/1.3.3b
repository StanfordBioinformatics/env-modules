#%Module1.0
#
## stringtie modulefile
##
## Initializes stringtie
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the stringtie package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes stringtie package"

# for Tcl script use only
set version 1.3.3b

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/stringtie/stringtie-1.3.3b.Linux_x86_64/
