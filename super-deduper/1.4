#%Module1.0
#
## super_deduper modulefile
##
## Initializes super_deduper
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the super-deduper package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes super-deduper package"

# for Tcl script use only
set version 1.4

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/super-deduper/$version/
