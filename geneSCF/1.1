#%Module1.0
#
## geneSCF modulefile
##
## Initializes geneSCF
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use geneSCF"
	puts stderr "\n\tVersion $version\n"
}

module-whatis "initializes geneSCF"

# for Tcl script use only
set geneSCFversion 1.1

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/geneSCF/$geneSCFversion/
