#%Module1.0
#
## csem modulefile
##
## Initializes csem
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the csem package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes csem package"

# for Tcl script use only
set version 2.4

# sets the MODULESAPPSDIR env var
module add modsappsdir

prepend-path PATH $::env(MODULESAPPSDIR)/csem/$version
