#%Module1.0
#
## trinity modulefile
##
## Initializes presto
##
proc ModulesHelp { } {
	global version

	puts stderr "\tInitializes your environment to use the presto python package"
	puts stderr "\n\tVersion $version\n"
}

#module-whatis "initializes trinity package"

# for Tcl script use only
set version 0.5.2

# sets the MODULESAPPSDIR env var
module add modsappsdir
module add python/3.4

prepend-path PATH $::env(MODULESAPPSDIR)/presto/$version/bin
